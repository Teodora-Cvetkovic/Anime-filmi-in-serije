      <table border="0" cellpadding="0" cellspacing="0" width="100%">
        <tr>
                                              <td valign="top" width="60">
              <div class="picSurround">
                <a href="https://myanimelist.net/profile/hoan_dan">
                  <img src="https://cdn.myanimelist.net/images/userimages/3755835.jpg?t=1419892200" data-src="https://cdn.myanimelist.net/images/userimages/3755835.jpg?t=1419892200" border="0" class=" lazyloaded" width="48">
                </a>
              </div>
            </td>
                    <td valign="top">
                          <a href="https://myanimelist.net/profile/hoan_dan">hoan_dan</a>
              <small>(<a href="/profile/hoan_dan/reviews">All reviews</a>)</small><br>
