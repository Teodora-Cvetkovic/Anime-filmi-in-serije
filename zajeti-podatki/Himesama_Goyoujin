      <table border="0" cellpadding="0" cellspacing="0" width="100%">
        <tr>
                                              <td valign="top" width="60">
              <div class="picSurround">
                <a href="https://myanimelist.net/profile/Karlo_C">
                  <img src="https://cdn.myanimelist.net/images/userimages/66067.jpg?t=1230101400" data-src="https://cdn.myanimelist.net/images/userimages/66067.jpg?t=1230101400" border="0" class=" lazyloaded" width="48">
                </a>
              </div>
            </td>
                    <td valign="top">
                          <a href="https://myanimelist.net/profile/Karlo_C">Karlo_C</a>
              <small>(<a href="/profile/Karlo_C/reviews">All reviews</a>)</small><br>
