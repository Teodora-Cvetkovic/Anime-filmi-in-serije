<script>
var path = location.pathname;
var param = location.search;

dataLayer = dataLayer || [];

// login.php

function ga_notlogin(){
  ga('send', 'event', 'not_login', '2.', '/login.php?from=%2Faccount%2Fmembership%2Fpayment%3Fplan%3D1&c-ms-mode=1');
}

function ga_sns(){
  ga('send', 'event', 'sns_login', '2-1-1.', '/login.php?login-mode=social&c-ms-mode=1');
}

function ga_login(){
  ga('send', 'event', 'id_login', '2-1-2.ID', '/login.php?login-mode=id&c-ms-mode=1');
}

// register.php

function ga_registration(){
  ga('send', 'event', 'registration', '2-2.', '/register.php?from=%2Faccount%2Fmembership%2Fpayment%3Fplan%3D1&c-ms-mode=2');
}

if ( path.match("register.php") && document.getElementById('ga_goodresult') ) {
    dataLayer.push({
        'trackPageview': '/register.php?from=%2F&c-ms-mode=2',
        'member_title': '2-2-1.',
        'event': 'loadready-register'
    });
}

if ( path.match("login.php") && document.getElementById('ga_goodresult') ) {
    dataLayer.push({
        'trackPageview': path.concat(param, "&c-ms-mode=2"),
        'member_title': '2-2-1.',
        'event': 'loadready-login'
    });
}

